#include <iostream>
#include<bits/stdc++.h> 
using namespace std;

int main()
{
    int count = 0,max_val=-999999999,max_word=-999999999;
    string str,sw="",ls="",lw="",sw2="",lw2="";
    getline(cin, str);
    str.erase(0, 1);
    str.pop_back();
    for(int i = 0; i<str.length();i++){
        if(str[i] == '\''){
            sw="",count=0;
            i++;
            while(str[i] != '\''){
                sw = sw + str[i];
                i++;
                count++;
            }
            if(count > max_val){
                max_val = count;
                ls = sw;
            }
            i++;
            
        }
        
    }
    count = 0;
    for(int j = 0;j < ls.length();j++){
        if(ls[j] != ' '){
        sw2 = sw2 + ls[j];
        }
        count++;
        if(ls[j+1] == ' ' || ls[j+1] == '\0'){ 
            if(count > max_word){
                max_word = count;
                if(sw2 == lw){
                    reverse(sw2.begin(),sw2.end());
                    lw = sw2;
                }else{
                    lw = sw2;
                }
            }
            sw2 = "";
            count = 0;
        } 
        
    }
    cout << ls << endl;
    cout << lw;
    return 0;
}
