#include <iostream>

using namespace std;

int main()
{
    int m,n,k,a,b,count=0,max_num=-999999999;
    cin >> m >> n >> k;
    int A[m][n];
    for(int i = 0; i < m; i++){
    for(int j = 0; j < n; j++){
        A[i][j] = 0;
    }
    cout << endl;
}
    for (int l = 0;l<k;l++){
        cin >> a >> b;
        a=a-1;
        b=b-1;
        A[a][b] = 1;
    }
    for(int i = 0; i < m; i++){
    for(int j = 0; j < n; j++){
        if(A[i][j]==1){
            if(A[i-1][j]==1 || A[i][j-1]==1 || A[i+1][j]==1 || A[i][j+1]==1 || A[i+1][j-1]==1 || A[i+1][j+1]==1 ||
              A[i-1][j-1]==1 ||  A[i-1][j+1]==1){
                count++;
            }
            
        }else{
            if(A[i-1][j]==0 || A[i][j-1]==0 || A[i+1][j]==0 || A[i][j+1]==0 || A[i+1][j-1]==0 || A[i+1][j+1]==0 ||
              A[i-1][j-1]==0 ||  A[i-1][j+1]==0){
                count++;
            }
        }
    }
     for (auto &row : A)
    {
        for (auto &column : row)
        {
            cout << column << " ";
        }
        cout << endl;
    }

    return 0;
}
