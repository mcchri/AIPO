#include <iostream>
#include <cmath>
#include <algorithm>
using namespace std;

int main()
{
    int c,n,ok=0,count=0;
    string sts,cs="",ns="";
    getline(cin, sts);
    for(int i = 0;i<sts.length();i++){
        if(ok == 0 && sts[i] != ' '){
            cs = cs + sts[i];
        }else if(sts[i] == ' '){
            ok++;
        }else{
            ns = ns + sts[i];
        }
    }
        c = stoi(cs);
        n = stoi(ns);
        int beg=0;
        beg = c;
        for(int i = c; i <= n;i++){
            int a = i;
            if(i%c==0){
                for(int j = beg;j<=n;j++){
                    int b = j;
                    int gc = __gcd(a, b);
                    if(gc == c){
                        count++;
                    }
                }
                beg+=beg;
            }
        }
        cout << count << endl;
        return 0;
    }
